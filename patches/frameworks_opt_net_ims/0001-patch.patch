+
+            case SESSION_EVENT_PEER_SIZE_CHANGED:
+                Log.d(TAG, "postEventFromNative : msg = SESSION_EVENT_PEER_SIZE_CHANGED");
+
+                vp.changePeerDimensions(arg1, arg2);
