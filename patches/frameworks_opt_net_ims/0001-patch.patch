+                mWfcCallOngoing = false;
+                displayWfcRegistrationNotification(false);
+            }
+        } else if (phoneType == RILConstants.GSM_PHONE && mWfcCallOngoing) {
+            mWfcCallOngoing = false;
+            displayWfcRegistrationNotification(false);
+        }
+    }
