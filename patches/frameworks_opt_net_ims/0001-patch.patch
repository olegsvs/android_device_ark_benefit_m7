+     * @hide
+     */
+    public void writeToParcel(Parcel dest, int flags) {
+        dest.writeLong(warnSize);
+        dest.writeLong(maxSize);
+        dest.writeInt(autoAcceptMode ? 1 : 0);
+        dest.writeInt(fileIcon ? 1 : 0);
+        dest.writeLong(maxFileIconSize);
+    }
