+    private void instantiateFromXmlNode(Node domNode) {
+        Element domElement = (Element) domNode;
+
+        NodeList node = domElement.getElementsByTagName(TAG_XCAP_ROOT);
+        if (node.getLength() > 0) {
+            mXcapRoot = ((Element) node.item(0)).getTextContent();
+        }
