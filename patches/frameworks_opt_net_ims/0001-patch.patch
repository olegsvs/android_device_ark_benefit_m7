+                updateDefaultId();
+
+                vp.handleCallSessionEvent(msg);
+                break;
+
+            case SESSION_EVENT_ERROR_SERVER_DIED:
+                Log.d(TAG, "postEventFromNative : msg = MSG_ERROR_SERVER_DIED");
