+        public static int getDefaultVoltePhoneId() {
+            int phoneId = SystemProperties.getInt(PhoneConstants.PROPERTY_CAPABILITY_SWITCH, 1) - 1;
+            if (phoneId < 0 || phoneId >= TelephonyManager.getDefault().getPhoneCount()) {
