+    private RILRequest findAndRemoveRequestFromList(int serial) {
+        RILRequest rr = null;
+        synchronized (mRequestList) {
+            rr = mRequestList.get(serial);
+            if (rr != null) {
+                mRequestList.remove(serial);
+            }
+        }
+
+        return rr;
+    }
