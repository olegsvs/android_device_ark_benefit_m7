+        return mTotal;
+    }
+
+    public int getUnused() {
+        return (mTotal - mUsed);
+    }
+
+    public void reset() {
+        mUsed = 0;
+        mTotal = 0;
+    }
+
+    public int describeContents() {
+        return 0;
+    }
+
+    public void writeToParcel(Parcel dest, int flags) {
